{
  "name": "@de./sdk-rn",
  "version": "1.0.30",
  "description": "Complete SDK pack for MSI React Native (Map Service Interface) and DClient services",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/",
    "src/",
    "README.md"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./types": {
      "types": "./dist/types/index.d.ts",
      "default": "./dist/types/index.d.ts"
    }
  },
  "author": "SCOREX Corporation",
  "private": false,
  "scripts": {
    "build:tsc": "rimraf ./dist && tsc",
    "build:copy-types": "mkdir -p dist/types && cp -r src/types/* dist/types/",
    "build": "yarn build:tsc && yarn build:copy-types",
    "compile": "yarn build",
    "prepack": "yarn run compile",
    "test": "echo 'Error: No test specified' && exit 1",
    "test:msi": "yarn build:tsc && jest test/msi.test.js",
    "test:client": "yarn build:tsc && jest test/client.test.js",
    "test:order": "yarn build:tsc && jest test/order.test.js",
    "test:event": "yarn build:tsc && jest test/event.test.js"
  },
  "dependencies": {
    "events": "^3.3.0",
    "socket.io-client": "^4.6.1",
    "webview.io": "^1.0.6"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-native": ">=0.60.0",
    "react-native-webview": ">=11.0.0"
  },
  "devDependencies": {
    "@types/events": "^3.0.3",
    "@types/jest": "^29.5.0",
    "@types/react": "^18.0.0",
    "@types/react-native": "^0.72.8",
    "expect.js": "^0.3.1",
    "jest": "^29.7.0",
    "react": "^18.2.0",
    "react-native": "^0.72.8",
    "react-native-webview": "^13.16.0",
    "rimraf": "^4.4.0",
    "ts-jest": "^29.0.5",
    "typescript": "^5.0.2"
  },
  "directories": {
    "dist": "dist/",
    "docs": "docs/",
    "test": "test/"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/de-dot/de.sdk-rn"
  },
  "keywords": [
    "react-native",
    "sdk",
    "map",
    "msi",
    "webview",
    "geolocation",
    "navigation",
    "routing",
    "real-time",
    "socket-io",
    "delidev",
    "mobile"
  ],
  "engines": {
    "node": ">=12.0.0"
  }
}
